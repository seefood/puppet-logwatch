{
  "name": "jonnyx-logwatch",
  "version": "1.0.0",
  "author": "jonnyx",
  "summary": "Manage Logwatch on Linux Servers",
  "license": "Apache-2.0",
  "source": "https://github.com/jonmosco/puppet-logwatch",
  "project_page": "https://github.com/jonmosco/puppet-logwatch",
  "issues_url": "https://github.com/jonmosco/puppet-logwatch/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
        "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
        "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
